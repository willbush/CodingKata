@model IEnumerable<string>

@Html.ActionLink("Home", "List", "Product", null,
new { @class = "btn btn-block btn-default btn-lg" })

@foreach (var link in Model) {
	var v = new {
			controller = "Product",
						action = "List",
						category = link,
						page = 1
		};
	@Html.RouteLink(link, v, new { @class = "btn btn-block btn-default btn-lg" })
}